-inherit base.plim

-block extrahead
  =parent.extrahead()
  =stylesheet(BASE + 'styles/command.styl')

-block scripts
  =parent.scripts()
  script src='https://cdnjs.cloudflare.com/ajax/libs/co/4.1.0/index.min.js'
  =script(BASE + 'scripts/command.coffee')
  =script('main.coffee')


header
  ol.breadcrumb
      li
        a href='${BASE}' Home
      li.active = title

  h1 = title

  -block header

main.console
  -block main
